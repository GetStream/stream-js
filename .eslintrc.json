{
    "plugins": [
    ],
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "no-console": 0,
        "no-mixed-spaces-and-tabs": 1,
        "comma-dangle": 0,
        "no-unused-vars": 1,
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            1,
            "always"
        ]
    },
    "env": {
        "es6": false,
        "browser": true,
        "commonjs": true,
        "mocha": true
    },
    "globals": {
        "process": true
    },
    "parserOptions": {
        "ecmaFeatures" : {
            "modules": true
        }
    }
}
